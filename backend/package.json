{
  "name": "backend",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ../generateGameData/index.js; node server.js",
    "dev": "node server.js"
  },
  "devDependencies": {
    "eslint": "^7.15.0",
    "eslint-config-google": "^0.14.0",
    "fastify-autoload": "^3.3.1",
    "fastify-sensible": "^3.1.0",
    "pino-pretty": "^4.3.0"
  },
  "dependencies": {
    "fastify": "^3.9.1",
    "node-html-parser": "^2.0.0",
    "node-html-to-image": "^3.1.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "commonjs": true,
      "es6": true
    },
    "extends": "google",
    "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
    },
    "rules": {
      "max-len": 0,
      "require-jsdoc": 0,
      "new-cap": 0,
      "no-unused-vars": 1,
      "linebreak-style": 0
    }
  }
}
